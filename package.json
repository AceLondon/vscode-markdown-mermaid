{
  "name": "markdown-mermaid",
  "version": "0.1.1",
  "displayName": "Markdown Preview Mermaid Support",
  "description": "Adds Mermaid diagram and flowchart support to VS Code's builtin markdown preview",
  "icon": "resources/logo.png",
  "keywords": [
    "mermaid",
    "markdown",
    "flowchart",
    "diagram",
    "multi-root ready"
  ],
  "publisher": "bierner",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mjbvz/vscode-markdown-mermaid.git"
  },
  "bugs": {
    "url": "https://github.com/mjbvz/vscode-markdown-mermaid/issues"
  },
  "engines": {
    "vscode": "^1.15.0"
  },
  "activationEvents": [],
  "main": "./index.js",
  "categories": [
    "Other"
  ],
  "contributes": {
    "markdown.previewStyles": [
      "./mermaid-markdown.css"
    ],
    "markdown.previewScripts": [
      "./page.bundle.js"
    ],
    "markdown.markdownItPlugins": true
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "mermaid": "7.0.x",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.5.3",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "webpack-cli",
    "generate-css": "node build/create-css.js"
  }
}
